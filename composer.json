  "keywords": [
    "statscore"
  ],
  "require": {
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "itav/serializer": "^2.0",
    "symfony/http-foundation": "^4.3"
  },
  "require-dev": {
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^8.2"
  },
  "type": "library",
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-0": {
      "Statscore": "lib/"
    },
    "psr-4": {
      "Statscore\\": "lib/Statscore/",
      "UnitTests\\": "tests/unit"
    }
  }
