  "homepage": "https://github.com/statscore/apiclient",
  "license": "GNU",
  "authors": [
    {
      "name": "Micha≈Ç Per",
      "email": "m.per@statscore.com",
      "homepage": "https://github.com/michalper",
      "role": "Chief Technical Officer"
    }
