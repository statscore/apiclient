    "guzzlehttp/guzzle": "^6.5",
    "symfony/http-foundation": "^5.0",
    "symfony/property-access": "^5.0",
    "symfony/property-info": "^5.0",
    "symfony/serializer": "^5.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^8.5",
    "symfony/var-dumper": "^5.0"
